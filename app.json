{
  "expo": {
    "name": "Momenel - Privacy First Social Network",
    "slug": "privacy-first-social-network",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "privacy": "unlisted",
    "userInterfaceStyle": "automatic",
    "description": "Momenel - Your Data-Free, Open Source Social.",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/45a1b78d-b958-4078-9413-4e1dfd455925"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.momenel.privacyfirstsocialnetwork"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 1,
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.momenel.privacyfirstsocialnetwork"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Momenel to access your photos. This is required to upload photos to your posts."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "45a1b78d-b958-4078-9413-4e1dfd455925"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
